{
  "version": 2,
  "functions": {
    "api/index.ts": {
      "runtime": "@vercel/node@3.0.0"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.ts"
    },
    {
      "src": "/(.*\\.js|.*\\.css|.*\\.png|.*\\.ico|.*\\.svg|.*\\.json)",
      "dest": "/client/dist/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/client/dist/index.html"
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "client/dist",
  "env": {
    "NODE_ENV": "production"
  }
}
